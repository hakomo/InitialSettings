key C-*M = ~C-*Enter
key C-H = BackSpace
key C-S-H = Delete
key C-U = A-BackQuote
key C-S-U = Apps

key C-*_1 = ~C-*F1
key C-*_2 = ~C-*F2
key C-*_3 = ~C-*F3
key C-*_4 = ~C-*F4
key C-*_5 = ~C-*F5
key C-*_6 = ~C-*F6
key C-*_7 = ~C-*F7
key C-*_8 = ~C-*F8
key C-*_9 = ~C-*F9
key C-*_0 = ~C-*F10
