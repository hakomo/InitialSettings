include '104.mayu'

mod control += !Enter

mod shift = !!Space
key R-*Space = &Ignore

mod alt += !!LeftShift !!E0RightShift
key R-*LeftShift = &Ignore
key R-*E0RightShift = &Ignore

key *LeftShift = &ShellExecute(, '..\\launcher', , , ShowNormal)
key *E0RightShift = &ShellExecute(, '..\\launcher', , , ShowNormal)

key *Escape = *BackQuote
